{
  "name": "@telia/styleguide-web",
  "version": "1.28.16",
  "private": false,
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "run-s clean generate-static-data generate-component-docs build:css && parcel build src/index.html && npm run generate-zip",
    "build:component-lib": "cd component-lib && npm install && npm run build",
    "build:css": "postcss src/css/index.pcss -o ./src/css/index.css",
    "clean": "npm run rimraf -- dist && mkdirp dist -p",
    "css:watch:component-lib": "cd component-lib && npm run css:watch",
    "css:watch:sg": "postcss src/css/index.pcss -o ./src/css/index.css --watch",
    "deploy:prod": "npm run git:checkout-master-and-pull && git push heroku master",
    "dev": "run-s clean generate-static-data generate-component-docs && run-p dev:server css:watch:component-lib css:watch:sg",
    "dev:server": "cross-env NODE_ENV=development LOCAL_DEV=true node server.js",
    "git:checkout-master-and-pull": "git checkout master && git pull --rebase",
    "generate-component-docs": "react-docgen ./component-lib/src --out ./src/component-docs.json --extension [jsx] --exclude index.js --exclude utils.js --pretty",
    "generate-static-data": "node scripts/generate-static-data.js",
    "generate-zip": "node scripts/generate-zip-file.js",
    "heroku-prebuild": "cross-env NODE_ENV=production && npm run build:component-lib",
    "heroku-postbuild": "npm run build",
    "lint": "eslint ./component-lib/src ./src --ext .jsx,.js",
    "lint:fix": "eslint --fix ./component-lib/src ./src --ext .jsx,.js",
    "rimraf": "rimraf",
    "start": "node server.js"
  },
  "author": "Telia Norge",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "archiver": "3.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "cross-env": "5.2.0",
    "eslint": "5.15.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-react": "7.12.4",
    "marked": "0.6.1",
    "npm-run-all": "4.1.5",
    "postcss-cli": "6.1.1",
    "postcss-custom-properties": "8.0.9",
    "postcss-easy-import": "3.0.0",
    "postcss-functions": "3.0.0",
    "postcss-nested": "4.1.2",
    "postcss-replace": "1.0.5",
    "svg4everybody": "2.1.9"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "cp-cli": "2.0.0",
    "express": "4.16.4",
    "lodash": "4.17.11",
    "lottie-web": "^5.5.0",
    "mkdirp": "0.5.1",
    "object-fit-images": "3.2.4",
    "parcel-bundler": "1.12.3",
    "picturefill": "3.0.3",
    "pretty": "2.0.0",
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-docgen": "4.0.1",
    "react-dom": "16.6.3",
    "react-hot-loader": "3.0.0-beta.7",
    "react-router-dom": "4.3.1",
    "react-simpletabs": "0.7.0",
    "rimraf": "2.6.3"
  },
  "alias": {
    "component-lib": "./component-lib/src",
    "component-lib-root": "./component-lib"
  },
  "engines": {
    "node": "10.1.0",
    "npm": "5.6.x"
  }
}
