{
  "name": "@telia/styleguide-web",
  "version": "1.9.5",
  "private": false,
  "description": "",
  "main": "server/server.js",
  "scripts": {
    "build": "run-s clean && webpack --config ./src/webpack-prod.config.js -p && run-s build-css build-server",
    "build-css": "npm run build-css:components && npm run build-css:custom",
    "build-css:components": "postcss --config postcss.config.js component-lib/src/index.pcss -o dist/public/css/bundle.components.css",
    "build-css:custom": "postcss --config postcss.config.js src/client/css/index.pcss -o dist/public/css/bundle.custom.css",
    "build-server": "babel ./src/server --out-dir ./dist/server --copy-files && npm run copy-assets",
    "copy-assets": "cp-cli src/public dist/public && cp-cli component-lib/assets dist/public",
    "clean": "npm run rimraf --  ./src/public/*/bundle.* dist && mkdirp dist -p",
    "css:components:watch": "postcss --config postcss.config.js component-lib/src/index.pcss -o dist/public/css/bundle.components.css --watch",
    "css:custom:watch": "postcss --config postcss.config.js src/client/css/index.pcss -o dist/public/css/bundle.custom.css --watch",
    "cp-cli": "cp-cli",
    "deploy:prod": "git push heroku master && babel-node deploy-slack.js",
    "dev": "run-s clean copy-assets && run-p start-dev-server start-server css:components:watch css:custom:watch",
    "heroku-postbuild": "npm run build",
    "lint": "eslint ./component-lib/src ./src --ext .jsx,.js",
    "lint:fix": "eslint --fix ./component-lib/src ./src --ext .jsx,.js",
    "start": "node dist/server/server.js",
    "start-server": "babel-node ./src/server/server.js",
    "start-dev-server": "webpack-dev-server --port 8090 --config ./src/webpack-dev.config.js",
    "test": "mocha --opts src/test/mocha.opts --compilers js:babel-core/register src/test",
    "test:watch": "nodemon --watch test/ --watch src/ --ext js,jsx --exec \"npm --silent run test\"",
    "rimraf": "rimraf"
  },
  "author": "Telia Norge",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-eslint": "7.2.3",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.1.2",
    "css-loader": "0.28.7",
    "cssnano": "3.10.0",
    "eslint": "4.19.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-react": "7.8.2",
    "extract-text-webpack-plugin": "3.0.1",
    "git-user-name": "1.2.0",
    "mocha": "4.0.1",
    "nodemon": "1.12.1",
    "npm-run-all": "4.1.1",
    "postcss": "6.0.22",
    "postcss-calc": "6.0.0",
    "postcss-cli": "4.1.1",
    "postcss-custom-properties": "6.1.0",
    "postcss-easy-import": "3.0.0",
    "postcss-functions": "3.0.0",
    "postcss-loader": "2.0.6",
    "postcss-nested": "^2.1.2",
    "postcss-replace": "^1.0.3",
    "postcss-url": "^7.3.0",
    "style-loader": "0.19.0",
    "webpack": "3.12.0",
    "webpack-dev-server": "2.11.1"
  },
  "dependencies": {
    "@telia/styleguide": "file:component-lib",
    "archiver": "2.0.3",
    "babel-standalone": "^6.26.0",
    "body-parser": "1.18.2",
    "brace": "^0.11.1",
    "classnames": "2.2.5",
    "cp-cli": "1.1.2",
    "ejs": "2.5.7",
    "express": "4.16.1",
    "file-loader": "1.1.5",
    "js-beautify": "^1.7.5",
    "lodash": "4.17.4",
    "marked": "0.3.9",
    "mkdirp": "0.5.1",
    "object-fit-images": "3.2.3",
    "picturefill": "3.0.2",
    "pretty": "2.0.0",
    "prop-types": "15.6.0",
    "proxy-middleware": "0.15.0",
    "raw-loader": "^0.5.1",
    "react": "16.3.2",
    "react-ace": "^6.1.1",
    "react-docgen": "2.18.0",
    "react-dom": "16.3.2",
    "react-hot-loader": "3.0.0-beta.7",
    "react-router-dom": "4.2.2",
    "react-simpletabs": "0.7.0",
    "rimraf": "^2.6.2",
    "whatwg-fetch": "2.0.3"
  },
  "engines": {
    "node": "10.1.0",
    "npm": "5.6.x"
  }
}
