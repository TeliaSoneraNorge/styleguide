{
  "name": "@telia/styleguide",
  "version": "1.33.48",
  "private": true,
  "description": "",
  "main": "dist/index.js",
  "jsnext:main": "dist/es/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "icons": "run-s icons:add-icons icons:svg-join icons:allicons-react",
    "icons:allicons-react": "node generate-AllIcons-component.js && babel ./assets/allicons/AllIcons.jsx.generated --out-file ./assets/allicons/AllIcons.js && cross-env BABEL_ENV=commonjs babel ./assets/allicons/AllIcons.jsx.generated --out-file ./assets/allicons/AllIcons.cjs.js",
    "icons:svg-join": "svg-join -s \"assets/icons-legacy/*.svg\" -o ./assets/allicons -n allicons  --mono",
    "icons:add-icons": "node ./scripts/add-icons.js",
    "build:icons-zip": "node ./scripts/generate-icons-zip-file.js",
    "build:zip": "node ./scripts/generate-zip-file.js",
    "build:icons": "node scripts/generate-icons.js",
    "build": "run-s test && run-p build:commonjs build:es build:css build:ts-types && run-s build:copy-files",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --extensions '.js,.jsx,.ts,.tsx' --out-dir dist",
    "build:copy-files": "node ./scripts/copy-files.js",
    "build:css": "postcss src/index.pcss src/business.pcss --dir dist --ext css --config ./postcss.config.js",
    "build:es": "cross-env BABEL_ENV=es babel src --extensions '.js,.jsx,.ts,.tsx' --out-dir dist/es",
    "build:ts-types": "run-s build:ts-emit-declarations build:ts-copy-type-files",
    "build:ts-emit-declarations": "./node_modules/.bin/tsc --build tsconfig.dist.json",
    "build:ts-copy-type-files": "node ./scripts/copy-type-files.js",
    "clean": "rimraf dist",
    "deploy:prod": "npm run build && npm publish dist",
    "deploy:beta": "npm run build && npm publish dist --tag beta",
    "dev:es": "cross-env BABEL_ENV=es babel src --extensions '.js,.jsx,.ts,.tsx' --out-dir dist/es --watch",
    "lint": "eslint --ext .jsx,.js ./src",
    "prebuild": "run-s clean",
    "publish:local-test": "npm run build && cd dist && npm pack",
    "css:watch": "cross-env LOCAL_DEV=true postcss src/index.pcss -o dist/index.css --watch --config ./postcss.config.js",
    "svgo:pebbles": "svgo -f ./assets/pebbles -o ./assets/pebbles-optimized --config=./svgo.config.yml --multipass",
    "storybook": "start-storybook -s ./assets -p 6006",
    "build-storybook": "cross-env STORYBOOK=true build-storybook -o dist-storybook",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Telia Norge",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.4.5",
    "@storybook/addon-actions": "^6.0.12",
    "@storybook/addon-docs": "^6.0.12",
    "@storybook/addon-knobs": "^6.0.12",
    "@storybook/addon-links": "^6.0.12",
    "@storybook/addons": "^6.0.12",
    "@storybook/react": "^6.0.12",
    "@telia/svg-join": "^1.0.1",
    "@types/classnames": "^2.2.9",
    "@types/lodash": "^4.14.144",
    "@types/pretty": "^2.0.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-preval": "^3.0.1",
    "babel-plugin-transform-dev-warning": "^0.1.1",
    "babel-plugin-transform-react-constant-elements": "^7.0.0-beta.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "core-js": "^3.6.5",
    "cross-env": "^5.2.0",
    "eslint": "4.18.2",
    "eslint-plugin-react": "7.4.0",
    "fork-ts-checker-webpack-plugin": "^4.0.4",
    "fs-extra": "^7.0.1",
    "jest": "^24.9.0",
    "js-yaml": "^3.13.1",
    "markdown-loader": "^5.1.0",
    "npm-run-all": "4.1.1",
    "postcss": "7.0.14",
    "postcss-calc": "7.0.1",
    "postcss-cli": "6.1.1",
    "postcss-custom-properties": "8.0.9",
    "postcss-easy-import": "3.0.0",
    "postcss-functions": "3.0.0",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.1",
    "postcss-replace": "1.0.5",
    "postcss-svg": "3.0.0",
    "postcss-url": "8.0.0",
    "pretty": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.11.0",
    "rimraf": "^2.6.3",
    "storybook-addon-jsx": "^7.3.4",
    "svg4everybody": "^2.1.9",
    "svgo": "^1.2.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "classnames": "2.2.5",
    "lodash": "^4.17.19",
    "lottie-web": "^5.5.5"
  },
  "sideEffects": false,
  "peerDependencies": {
    "prop-types": "15.x",
    "react": "16.x",
    "react-dom": "16.x"
  },
  "workspaces": [
    "src/*"
  ],
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.test.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "OLD": {
    "name": "@telia/styleguide-web",
    "version": "1.33.48",
    "private": false,
    "description": "",
    "main": "server.js",
    "scripts": {
      "build": "run-s clean generate-static-data generate-component-docs build:css && parcel build src/index.html",
      "build:component-lib": "cd component-lib && npm install && npm run build",
      "build:css": "postcss src/css/index.pcss -o ./src/css/index.css",
      "clean": "npm run rimraf -- dist && mkdirp dist -p",
      "css:watch:sg": "postcss src/css/index.pcss -o ./src/css/index.css --watch",
      "css:watch:component-lib": "cd component-lib && npm run css:watch",
      "dev": "run-s clean generate-static-data generate-component-docs && run-p dev:server css:watch:component-lib css:watch:sg",
      "dev:server": "cross-env NODE_ENV=development LOCAL_DEV=true node server.js",
      "git:checkout-master-and-pull": "git checkout master && git pull --rebase",
      "generate-component-docs": "react-docgen ./component-lib/src --out ./src/component-docs.json --extension [jsx] --exclude index.js --exclude utils.js --exclude constants.js --exclude useThSortable.js --pretty",
      "generate-static-data": "node scripts/generate-static-data.js",
      "lint": "eslint ./component-lib/src ./src --ext .jsx,.js",
      "lint:fix": "eslint --fix ./component-lib/src ./src --ext .jsx,.js",
      "rimraf": "rimraf",
      "start": "node server.js"
    },
    "author": "Telia Norge",
    "license": "ISC",
    "devDependencies": {
      "@babel/cli": "^7.4.4",
      "@babel/core": "7.3.4",
      "@babel/plugin-proposal-class-properties": "7.3.4",
      "@babel/plugin-proposal-optional-chaining": "^7.10.1",
      "@babel/preset-env": "7.3.4",
      "@babel/preset-react": "7.0.0",
      "@types/node": "^13.9.1",
      "archiver": "3.0.0",
      "babel-eslint": "10.0.1",
      "babel-loader": "8.0.5",
      "babel-plugin-external-helpers": "6.22.0",
      "babel-plugin-transform-react-jsx": "6.24.1",
      "cross-env": "5.2.0",
      "eslint": "^5.16.0",
      "eslint-loader": "2.1.2",
      "eslint-plugin-react": "^7.13.0",
      "husky": "^2.4.1",
      "lint-staged": "^8.2.1",
      "marked": "^0.7.0",
      "npm-run-all": "4.1.5",
      "parcel-plugin-static-files-copy": "^2.1.2",
      "postcss-cli": "6.1.1",
      "postcss-custom-properties": "8.0.9",
      "postcss-easy-import": "3.0.0",
      "postcss-functions": "3.0.0",
      "postcss-nested": "4.1.2",
      "postcss-replace": "1.0.5",
      "postcss-url": "^8.0.0",
      "prettier": "1.18.2",
      "svg4everybody": "2.1.9"
    },
    "dependencies": {
      "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
      "assets": "^3.0.1",
      "classnames": "2.2.6",
      "cp-cli": "2.0.0",
      "express": "4.16.4",
      "lodash": "^4.17.15",
      "lottie-web": "^5.5.5",
      "mkdirp": "0.5.1",
      "object-fit-images": "3.2.4",
      "parcel-bundler": "1.12.4",
      "picturefill": "3.0.3",
      "pretty": "2.0.0",
      "prop-types": "15.7.2",
      "react": "^16.8.6",
      "react-docgen": "4.0.1",
      "react-dom": "^16.8.6",
      "react-hot-loader": "3.0.0-beta.7",
      "react-router-dom": "4.3.1",
      "react-router-hash-link": "^1.2.2",
      "rimraf": "2.6.3"
    },
    "alias": {
      "@telia/styleguide": "./component-lib/src",
      "component-lib": "./component-lib/src",
      "component-lib-root": "./component-lib",
      "assets": "./src/assets",
      "react": "./node_modules/react"
    },
    "engines": {
      "node": ">=10.1.0",
      "npm": "5.6.x"
    },
    "staticFiles": {
      "staticPath": [
        "component-lib/assets/pebbles"
      ]
    }
  }
