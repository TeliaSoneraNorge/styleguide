{
  "name": "@telia/styleguide",
  "version": "1.26.0-beta-1",
  "private": false,
  "description": "",
  "scripts": {
    "allicons:react": "node generate-AllIcons-component.js && babel ./assets/allicons/AllIcons.jsx.generated --out-file ./assets/allicons/AllIcons.js",
    "build": "npm run remove-dist && run-p build:library:css build:library:js && npm run svgo && npm run svg-join && npm run allicons:react",
    "build:copy-files": "babel-node --config-file ./babel.config.js ./scripts/copy-files.js",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src --out-dir ./dist",
    "build:library:css": "postcss src/index.pcss -o dist/index.css --config ./postcss.config.js",
    "build:library:js": "npm run build:es && npm run build:copy-files",
    "build:library:watch": "run-p build:library:css:watch build:library:js:watch svgo",
    "build:library:css:watch": "postcss src/index.pcss -o dist/index.css --watch --config ./postcss.config.js",
    "deploy:prod": "npm run build && npm publish dist",
    "deploy:beta": "npm run build && npm publish dist --tag beta",
    "lint": "eslint --ext .jsx,.js /",
    "prebuild": "npm run remove-dist",
    "publish:local-test": "npm run build && cd dist && npm pack",
    "remove-dist": "npm run rimraf -- dist",
    "rimraf": "rimraf",
    "svgo": "svgo -f ./assets/unoptimized-icons -o ./assets/icons --config=./svgo.config.yml --multipass",
    "svgo:pebbles": "svgo -f ./assets/pebbles -o ./assets/pebbles-optimized --config=./svgo.config.yml --multipass",
    "svg-join": "svg-join -s \"assets/icons/*.svg\" -o ./assets/allicons -n allicons  --mono"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Telia Norge",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-preval": "^3.0.1",
    "babel-plugin-transform-dev-warning": "^0.1.1",
    "babel-plugin-transform-react-constant-elements": "^7.0.0-beta.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.20",
    "cross-env": "^5.2.0",
    "eslint": "4.8.0",
    "eslint-plugin-react": "7.4.0",
    "fs-extra": "^7.0.1",
    "npm-run-all": "4.1.1",
    "postcss": "7.0.14",
    "postcss-calc": "7.0.1",
    "postcss-cli": "6.1.1",
    "postcss-custom-properties": "8.0.9",
    "postcss-easy-import": "3.0.0",
    "postcss-functions": "3.0.0",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.1",
    "postcss-replace": "1.0.4",
    "postcss-svg": "3.0.0",
    "postcss-url": "8.0.0",
    "rimraf": "^2.6.2",
    "svg-join": "^1.1.1",
    "svg4everybody": "^2.1.9",
    "svgo": "^0.7.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "classnames": "2.2.5",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "sideEffects": false,
  "peerDependencies": {
    "prop-types": "15.x",
    "react": "16.x",
    "react-dom": "16.x"
  },
  "workspaces": [
    "src/*"
  ],
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.test.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
