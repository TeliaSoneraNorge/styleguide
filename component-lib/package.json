{
  "name": "@telia/styleguide",
  "version": "1.28.46",
  "private": true,
  "description": "",
  "main": "dist/index.js",
  "jsnext:main": "dist/es/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "build": "run-p build:commonjs build:es build:css build:icons && run-s build:copy-files",
    "build:allicons-react": "node generate-AllIcons-component.js && babel ./assets/allicons/AllIcons.jsx.generated --out-file ./assets/allicons/AllIcons.js",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir dist",
    "build:copy-files": "node ./scripts/copy-files.js",
    "build:css": "postcss src/index.pcss -o dist/index.css --config ./postcss.config.js",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir dist/es",
    "build:icons": "run-s build:svgo build:svg-join build:allicons-react",
    "build:svg-join": "svg-join -s \"assets/icons/*.svg\" -o ./assets/allicons -n allicons  --mono",
    "build:svgo": "svgo -f ./assets/unoptimized-icons -o ./assets/icons --config=./svgo.config.yml --multipass",
    "clean": "rimraf dist",
    "deploy:prod": "npm run build && npm publish dist",
    "deploy:beta": "npm run build && npm publish dist --tag beta",
    "lint": "eslint --ext .jsx,.js ./src",
    "prebuild": "run-s clean",
    "publish:local-test": "npm run build && cd dist && npm pack",
    "css:watch": "cross-env LOCAL_DEV=true postcss src/index.pcss -o dist/index.css --watch --config ./postcss.config.js",
    "svgo:pebbles": "svgo -f ./assets/pebbles -o ./assets/pebbles-optimized --config=./svgo.config.yml --multipass"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Telia Norge",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "@telia/svg-join": "^1.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-preval": "^3.0.1",
    "babel-plugin-transform-dev-warning": "^0.1.1",
    "babel-plugin-transform-react-constant-elements": "^7.0.0-beta.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^5.2.0",
    "eslint": "4.8.0",
    "eslint-plugin-react": "7.4.0",
    "fs-extra": "^7.0.1",
    "npm-run-all": "4.1.1",
    "postcss": "7.0.14",
    "postcss-calc": "7.0.1",
    "postcss-cli": "6.1.1",
    "postcss-custom-properties": "8.0.9",
    "postcss-easy-import": "3.0.0",
    "postcss-functions": "3.0.0",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.1",
    "postcss-replace": "1.0.5",
    "postcss-svg": "3.0.0",
    "postcss-url": "8.0.0",
    "rimraf": "^2.6.3",
    "svg4everybody": "^2.1.9",
    "svgo": "^1.2.2"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "lodash": "^4.17.15",
    "lottie-web": "^5.5.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "sideEffects": false,
  "peerDependencies": {
    "prop-types": "15.x",
    "react": "16.x",
    "react-dom": "16.x"
  },
  "workspaces": [
    "src/*"
  ],
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.test.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
