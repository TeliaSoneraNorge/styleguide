{
  "name": "@telia/styleguide",
  "version": "1.30.15",
  "private": true,
  "description": "",
  "main": "dist/index.js",
  "jsnext:main": "dist/es/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "icons": "run-s icons:add-icons icons:svg-join icons:allicons-react",
    "icons:allicons-react": "node generate-AllIcons-component.js && babel ./assets/allicons/AllIcons.jsx.generated --out-file ./assets/allicons/AllIcons.js && cross-env BABEL_ENV=commonjs babel ./assets/allicons/AllIcons.jsx.generated --out-file ./assets/allicons/AllIcons.cjs.js",
    "icons:svg-join": "svg-join -s \"assets/icons/*.svg\" -o ./assets/allicons -n allicons  --mono",
    "icons:add-icons": "node ./scripts/add-icons.js",
    "icons:add-business-icons": "node ./scripts/add-business-icons.js",
    "icons:business": "run-s icons:add-business-icons icons:svg-join-business icons:business-allicons-react",
    "icons:business-allicons-react": "node generate-AllBusinessIcons-component.js",
    "icons:generate-business-zip": "node ./scripts/generate-business-icons-zip-file.js",
    "icons:svg-join-business": "svg-join -s \"assets/business-icons/*.svg\" -o ./assets/business-allicons -n business-allicons -p Business-Icon-- --mono",
    "build": "run-s test && run-p build:commonjs build:es build:css build:ts-types && run-s build:copy-files",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --extensions '.js,.jsx,.ts,.tsx' --out-dir dist",
    "build:copy-files": "node ./scripts/copy-files.js",
    "build:css": "postcss src/index.pcss src/business.pcss --dir dist --ext css --config ./postcss.config.js",
    "build:es": "cross-env BABEL_ENV=es babel src --extensions '.js,.jsx,.ts,.tsx' --out-dir dist/es",
    "build:ts-types": "run-s build:ts-emit-declarations build:ts-copy-type-files",
    "build:ts-emit-declarations": "./node_modules/.bin/tsc --build tsconfig.dist.json",
    "build:ts-copy-type-files": "node ./scripts/copy-type-files.js",
    "clean": "rimraf dist",
    "deploy:prod": "npm run build && npm publish dist",
    "deploy:beta": "npm run build && npm publish dist --tag beta",
    "dev:es": "cross-env BABEL_ENV=es babel src --extensions '.js,.jsx,.ts,.tsx' --out-dir dist/es --watch",
    "lint": "eslint --ext .jsx,.js ./src",
    "prebuild": "run-s clean",
    "publish:local-test": "npm run build && cd dist && npm pack",
    "css:watch": "cross-env LOCAL_DEV=true postcss src/index.pcss -o dist/index.css --watch --config ./postcss.config.js",
    "svgo:pebbles": "svgo -f ./assets/pebbles -o ./assets/pebbles-optimized --config=./svgo.config.yml --multipass",
    "storybook": "start-storybook -s ./assets -p 6006",
    "build-storybook": "cross-env STORYBOOK=true build-storybook -o dist-storybook",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Telia Norge",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.4.5",
    "@storybook/addon-actions": "^5.3.3",
    "@storybook/addon-docs": "^5.3.3",
    "@storybook/addon-knobs": "^5.3.3",
    "@storybook/addon-links": "^5.3.3",
    "@storybook/addons": "^5.3.3",
    "@storybook/react": "^5.3.3",
    "@telia/svg-join": "^1.0.1",
    "@types/classnames": "^2.2.9",
    "@types/lodash": "^4.14.144",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-preval": "^3.0.1",
    "babel-plugin-transform-dev-warning": "^0.1.1",
    "babel-plugin-transform-react-constant-elements": "^7.0.0-beta.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^5.2.0",
    "eslint": "4.18.2",
    "eslint-plugin-react": "7.4.0",
    "fs-extra": "^7.0.1",
    "jest": "^24.9.0",
    "js-yaml": "^3.13.1",
    "npm-run-all": "4.1.1",
    "postcss": "7.0.14",
    "postcss-calc": "7.0.1",
    "postcss-cli": "6.1.1",
    "postcss-custom-properties": "8.0.9",
    "postcss-easy-import": "3.0.0",
    "postcss-functions": "3.0.0",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.1",
    "postcss-replace": "1.0.5",
    "postcss-svg": "3.0.0",
    "postcss-url": "8.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "rimraf": "^2.6.3",
    "storybook-addon-jsx": "^7.1.13",
    "svg4everybody": "^2.1.9",
    "svgo": "^1.2.2",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "lodash": "^4.17.15",
    "lottie-web": "^5.5.5"
  },
  "sideEffects": false,
  "peerDependencies": {
    "prop-types": "15.x",
    "react": "16.x",
    "react-dom": "16.x"
  },
  "workspaces": [
    "src/*"
  ],
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.test.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
